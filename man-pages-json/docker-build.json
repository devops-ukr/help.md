{
    "Command": "docker build",
    "Description": "Build an image from a Dockerfile",
    "Usage": "\tdocker build [OPTIONS] PATH | URL | -",
    "options": {
        "indent": 31,
        "data": [
            {
                "cmd": "      --add-host list          ",
                "dcr": " Add a custom host-to-IP mapping (host:ip)"
            },
            {
                "cmd": "      --build-arg list         ",
                "dcr": " Set build-time variables"
            },
            {
                "cmd": "      --cache-from strings     ",
                "dcr": " Images to consider as cache sources"
            },
            {
                "cmd": "      --cgroup-parent string   ",
                "dcr": " Optional parent cgroup for the container"
            },
            {
                "cmd": "      --compress               ",
                "dcr": " Compress the build context using gzip"
            },
            {
                "cmd": "      --cpu-period int         ",
                "dcr": " Limit the CPU CFS (Completely Fair Scheduler) period"
            },
            {
                "cmd": "      --cpu-quota int          ",
                "dcr": " Limit the CPU CFS (Completely Fair Scheduler) quota"
            },
            {
                "cmd": "  -c, --cpu-shares int         ",
                "dcr": " CPU shares (relative weight)"
            },
            {
                "cmd": "      --cpuset-cpus string     ",
                "dcr": " CPUs in which to allow execution (0-3, 0,1)"
            },
            {
                "cmd": "      --cpuset-mems string     ",
                "dcr": " MEMs in which to allow execution (0-3, 0,1)"
            },
            {
                "cmd": "      --disable-content-trust  ",
                "dcr": " Skip image verification (default true)"
            },
            {
                "cmd": "  -f, --file string            ",
                "dcr": " Name of the Dockerfile (Default is 'PATH\/Dockerfile')"
            },
            {
                "cmd": "      --force-rm               ",
                "dcr": " Always remove intermediate containers"
            },
            {
                "cmd": "      --iidfile string         ",
                "dcr": " Write the image ID to the file"
            },
            {
                "cmd": "      --isolation string       ",
                "dcr": " Container isolation technology"
            },
            {
                "cmd": "      --label list             ",
                "dcr": " Set metadata for an image"
            },
            {
                "cmd": "  -m, --memory bytes           ",
                "dcr": " Memory limit"
            },
            {
                "cmd": "      --memory-swap bytes      ",
                "dcr": " Swap limit equal to memory plus swap: '-1' to enable unlimited swap"
            },
            {
                "cmd": "      --network string         ",
                "dcr": " Set the networking mode for the RUN instructions during build (default \"default\")"
            },
            {
                "cmd": "      --no-cache               ",
                "dcr": " Do not use cache when building the image"
            },
            {
                "cmd": "      --platform string        ",
                "dcr": " Set platform if server is multi-platform capable"
            },
            {
                "cmd": "      --pull                   ",
                "dcr": " Always attempt to pull a newer version of the image"
            },
            {
                "cmd": "  -q, --quiet                  ",
                "dcr": " Suppress the build output and print image ID on success"
            },
            {
                "cmd": "      --rm                     ",
                "dcr": " Remove intermediate containers after a successful build (default true)"
            },
            {
                "cmd": "      --security-opt strings   ",
                "dcr": " Security options"
            },
            {
                "cmd": "      --shm-size bytes         ",
                "dcr": " Size of \/dev\/shm"
            },
            {
                "cmd": "      --squash                 ",
                "dcr": " Squash newly built layers into a single new layer"
            },
            {
                "cmd": "      --stream                 ",
                "dcr": " Stream attaches to server to negotiate build context"
            },
            {
                "cmd": "  -t, --tag list               ",
                "dcr": " Name and optionally a tag in the 'name:tag' format"
            },
            {
                "cmd": "      --target string          ",
                "dcr": " Set the target build stage to build."
            },
            {
                "cmd": "      --ulimit ulimit          ",
                "dcr": " Ulimit options (default [])"
            }
        ]
    }
}