{
    "Command": "docker plugin upgrade",
    "Description": "Upgrade an existing plugin",
    "Usage": "\tdocker plugin upgrade [OPTIONS] PLUGIN [REMOTE]",
    "options": {
        "indent": 31,
        "data": [
            {
                "cmd": "      --disable-content-trust  ",
                "dcr": " Skip image verification (default true)"
            },
            {
