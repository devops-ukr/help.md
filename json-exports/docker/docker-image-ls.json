{
    "Command": "docker image ls",
    "Description": "List images",
    "Usage": "\tdocker image ls [OPTIONS] [REPOSITORY[:TAG]]",
    "options": {
        "indent": 23,
        "data": [
            {
                "cmd": "-a, --all",
                "dcr": " Show all images (default hides intermediate images)"
            },
            {
                "cmd": "--digests",
                "dcr": " Show digests"
            },
            {
                "cmd": "-f, --filter filter",
                "dcr": " Filter output based on conditions provided"
            },
            {
                "cmd": "--format string",
                "dcr": " Pretty-print images using a Go template"
            },
            {
                "cmd": "--no-trunc",
                "dcr": " Don't truncate output"
            },
            {
                "cmd": "-q, --quiet",
                "dcr": " Only show numeric IDs"
            }
        ]
    }
}