{
    "Command": "docker container ls",
    "Description": "List containers",
    "Usage": "\tdocker container ls [OPTIONS]",
    "options": {
        "indent": 23,
        "data": [
            {
                "cmd": "-a, --all",
                "dcr": " Show all containers (default shows just running)"
            },
            {
                "cmd": "-f, --filter filter",
                "dcr": " Filter output based on conditions provided"
            },
            {
                "cmd": "--format string",
                "dcr": " Pretty-print containers using a Go template"
            },
            {
                "cmd": "-n, --last int",
                "dcr": " Show n last created containers (includes allstates) (default -1)"
            },
            {
                "cmd": "-l, --latest",
                "dcr": " Show the latest created container (includes allstates)"
            },
            {
                "cmd": "--no-trunc",
                "dcr": " Don't truncate output"
            },
            {
                "cmd": "-q, --quiet",
                "dcr": " Only display numeric IDs"
            },
            {
                "cmd": "-s, --size",
                "dcr": " Display total file sizes"
            }
        ]
    }
}