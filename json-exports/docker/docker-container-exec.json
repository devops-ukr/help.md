{
    "Command": "docker container exec",
    "Description": "Run a command in a running container",
    "Usage": "\tdocker container exec [OPTIONS] CONTAINER COMMAND [ARG...]",
    "options": {
        "indent": 28,
        "data": [
            {
                "cmd": "-d, --detach",
                "dcr": " Detached mode: run command in the background"
            },
            {
                "cmd": "--detach-keys string",
                "dcr": " Override the key sequence for detaching acontainer"
            },
            {
                "cmd": "-e, --env list",
                "dcr": " Set environment variables"
            },
            {
                "cmd": "-i, --interactive",
                "dcr": " Keep STDIN open even if not attached"
            },
            {
                "cmd": "--privileged",
                "dcr": " Give extended privileges to the command"
            },
            {
                "cmd": "-t, --tty",
                "dcr": " Allocate a pseudo-TTY"
            },
            {
                "cmd": "-u, --user string",
                "dcr": " Username or UID (format:<name|uid>[:<group|gid>])"
            },
            {
                "cmd": "-w, --workdir string",
                "dcr": " Working directory inside the container"
            }
        ]
    }
}