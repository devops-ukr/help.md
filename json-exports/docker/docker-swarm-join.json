{
    "Command": "docker swarm join",
    "Description": "Join a swarm as a node and\/or manager",
    "Usage": "\tdocker swarm join [OPTIONS] HOST:PORT",
    "options": {
        "indent": 31,
        "data": [
            {
                "cmd": "--advertise-addr string",
                "dcr": " Advertised address (format:<ip|interface>[:port])"
            },
            {
                "cmd": "--availability string",
                "dcr": " Availability of the node(\"active\"|\"pause\"|\"drain\") (default \"active\")"
            },
            {
                "cmd": "--data-path-addr string",
                "dcr": " Address or interface to use for data pathtraffic (format: <ip|interface>)"
            },
            {
                "cmd": "--listen-addr node-addr",
                "dcr": " Listen address (format:<ip|interface>[:port]) (default 0.0.0.0:2377)"
            },
            {
                "cmd": "--token string",
                "dcr": " Token for entry into the swarm"
            }
        ]
    }
}